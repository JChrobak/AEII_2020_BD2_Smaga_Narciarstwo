<Page x:Class="E_SkiLift.Windows.CompanySummaryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:E_SkiLift.Windows"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CompanySummaryPage">

    <Grid>
        <Canvas HorizontalAlignment="Left" Height="232" Margin="10,10,0,0" VerticalAlignment="Top" Width="578">
            <DatePicker Canvas.Left="65" Canvas.Top="34"/>
            <DatePicker Canvas.Left="200" Canvas.Top="34"/>
            <Label Content="From" Height="29" Canvas.Left="65" Canvas.Top="10" Width="102"/>
            <Label Content="To" Height="29" Canvas.Left="200" Canvas.Top="10" Width="102"/>
            <Label Content="Income" Canvas.Left="65" Canvas.Top="79"/>
            <Label Content="$10000000000.00" Canvas.Left="200" Canvas.Top="79"/>
            <Button x:Name="returnButton" Content="RETURN" Click="returnButton_Click" VerticalAlignment="Bottom" HorizontalAlignment="Left" Canvas.Left="27" Canvas.Top="212"/>
        </Canvas>
    </Grid>
</Page>
